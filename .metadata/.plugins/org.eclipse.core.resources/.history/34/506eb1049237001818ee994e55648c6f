object SpecialFunctions {
  ((x:Int) => x+1) (5)                            //> res0: Int = 6
  
  val f = ((x:Int) => x+1)                        //> f  : Int => Int = SpecialFunctions$$$Lambda$9/488970385@48140564
  f(5)                                            //> res1: Int = 6
  f.apply(3)                                      //> res2: Int = 4
  
  val doubler = (i:Int)=>{i*2}                    //> doubler  : Int => Int = SpecialFunctions$$$Lambda$10/93122545@7c30a502
  doubler(6)                                      //> res3: Int = 12
  
  def someOperation(f:(Int, Int)=>Int){
  	println(f(5,5))}                          //> someOperation: (f: (Int, Int) => Int)Unit
}